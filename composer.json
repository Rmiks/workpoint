    "vendor-dir": "shared/vendor/"
  },
  "require": {
    "php": ">=5.5.0",
    "phpoffice/phpexcel": "^1.8",
    "filp/whoops": "~2.1.0",
    "maximebf/debugbar": ">=1.0.0",
    "ps/image-optimizer": "^1.0",
    "swiftmailer/swiftmailer": "^5.4",
    "facebook/graph-sdk": "^5.4",
    "google/apiclient": "^2.0.3",
    "abraham/twitteroauth": "^0.7.0",
    "draugiem/draugiem-php-sdk": "^1.3",
    "bocharsky-bw/vkontakte-php-sdk": "^2.0",
